{
	"manifest_version":2,
	"name":"Hello World",
	"version":"1.0",
	"description":"A hello world extension!",
	"icons": {
		"128":"./images/128.png",
		"48":"./images/48.png",
		"16":"./images/16.png"
	},
	"browser_action":{
	"default_icon":"./images/16.png",
	"default_popup":"popup.html"
	},
	"permissions":[
		"clipboardRead",
		"clipboardWrite"
	],
	"content_scripts":[
		{
			"matches":[
				"<all_urls>"
			],
			"js":["pop.js"]
		}]
}